{
  "name": "films-search",
  "version": "1.0.0",
  "description": "Small application which goal is to search films",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "webpack-dev-server --hot --config ./webpack.config.js --mode development",
    "start:server": "node index.js",
    "build": "webpack --config ./webpack.config.js --mode production",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "prettier": "prettier --config .prettierrc --write ./src/**/*.js",
    "lint": "echo \\\"Error: no lint specified\\\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtChetya/Films-Search.git"
  },
  "author": "Artem Chetverikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArtChetya/Films-Search/issues"
  },
  "homepage": "https://github.com/ArtChetya/Films-Search#readme",
  "dependencies": {
    "express": "4.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.4",
    "redux-devtools": "^3.5.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "",
      "pre-commit": [
        "npm run precommit"
      ],
      "pre-push": [
        "npm run lint",
        "npm test"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
