{
  "name": "films-search",
  "version": "1.0.0",
  "description": "Small application which goal is to search films",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "webpack-dev-server --hot --config ./webpack.config.js --mode development",
    "start:server": "node index.js",
    "build:prod": "webpack --config ./webpack.config.js --mode production",
    "build:dev": "webpack --config ./webpack.config.js --mode development",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "prettier": "prettier --config .prettierrc --write ./src/**/*.js",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint ./src/**/*.js --fix",
    "precommit": "lint-staged",
    "prepush": "concurrently \"npm run lint\" \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtChetya/Films-Search.git"
  },
  "author": "Artem Chetverikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArtChetya/Films-Search/issues"
  },
  "homepage": "https://github.com/ArtChetya/Films-Search#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "express": "4.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.4.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.4",
    "redux-devtools": "^3.5.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "npm run prettier",
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "npm run precommit"
      ],
      "pre-push": [
        "npm run prepush"
      ]
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
